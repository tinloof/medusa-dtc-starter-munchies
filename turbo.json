{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "outputs": [
        "dist/**",
        ".medusa/**"
      ],
      "env": [
        "SANITY_STUDIO_PROJECT_ID",
        "SANITY_STUDIO_DATASET",
        "SANITY_TOKEN"
      ]
    },
    "//#format-and-lint": {},
    "//#format-and-lint:fix": {
      "cache": false
    },
    "check-types": {
      "dependsOn": [
        "^check-types"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "typegen": {
      "cache": true,
      "outputs": [
        "schema.json",
        "sanity.types.ts"
      ],
      "env": [
        "SANITY_STUDIO_DATASET",
        "SANITY_STUDIO_PROJECT_ID"
      ]
    },
    "@packages/sanity#typegen:watch": {
      "outputs": [
        "schema.json",
        "sanity.types.ts"
      ],
      "cache": false,
      "persistent": true
    },
    "@packages/sanity#build": {
      "dependsOn": [
        "@packages/sanity#typegen"
      ],
      "outputs": [
        "../../apps/web/public/cms/**",
        "./sanity/**"
      ],
      "cache": true,
      "env": [
        "SANITY_STUDIO_DATASET",
        "SANITY_STUDIO_PROJECT_ID"
      ]
    },
    "@packages/sanity#dev": {
      "with": [
        "@packages/sanity#typegen:watch"
      ],
      "persistent": true
    },
    "@apps/web#build": {
      "dependsOn": [
        "@packages/sanity#build"
      ],
      "outputs": [
        "dist/**"
      ],
      "cache": true,
      "env": [
        "SANITY_STUDIO_PROJECT_ID",
        "SANITY_STUDIO_DATASET",
        "SANITY_TOKEN"
      ]
    }
  }
}
