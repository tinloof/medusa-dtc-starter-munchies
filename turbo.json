{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "//#check": {},
    "//#fix": {
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**", ".medusa/**"],
      "env": [
        "SANITY_STUDIO_PROJECT_ID",
        "SANITY_STUDIO_DATASET",
        "SANITY_TOKEN"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "typegen": {
      "cache": true,
      "outputs": ["schema.json", "sanity.types.ts"],
      "env": ["SANITY_STUDIO_DATASET", "SANITY_STUDIO_PROJECT_ID"]
    },
    "@apps/web#typegen:watch": {
      "outputs": ["schema.json", "sanity.types.ts"],
      "cache": false,
      "persistent": true
    },
    "@apps/web#dev": {
      "with": ["@apps/web#typegen:watch"],
      "persistent": true
    },
    "@apps/web#build": {
      "outputs": ["dist/**"],
      "cache": true,
      "env": [
        "SANITY_STUDIO_PROJECT_ID",
        "SANITY_STUDIO_DATASET",
        "SANITY_TOKEN"
      ]
    }
  }
}
