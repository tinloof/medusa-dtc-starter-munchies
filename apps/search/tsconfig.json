{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "strict": true,
    "skipLibCheck": true,
    "types": ["@cloudflare/workers-types"],
    "lib": ["ESNext"],
    "noEmit": true,
    "isolatedModules": true
  },
  "include": ["src/**/*", "scripts/**/*"]
}
